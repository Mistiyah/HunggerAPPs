{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0
  },
  "plugins": ["jest", "import"],
  "ignorePatterns": ["dist/"]
}
